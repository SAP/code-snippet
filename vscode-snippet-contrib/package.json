{
	"name": "vscode-snippet-contrib",
	"displayName": "vscode-snippet-contrib",
	"description": "",
	"version": "0.0.2",
	"publisher": "SAPOSS",
	"engines": {
		"vscode": "^1.44.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/SAP/code-snippet",
		"directory": "vscode-snippet-contrib"
	},
	"extensionDependencies": [
		"saposs.code-snippet"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.showCodeSnippetContributor"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.showCodeSnippetContributor",
				"title": "Show Code Snippet Contributor"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.showCodeSnippetContributor",
					"group": "navigation@1",
					"when": "resourceFilename == launch.json"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"package": "vsce package ."
	},
	"dependencies": {
		"@sap-devx/code-snippet-types": "0.0.2",
		"lodash": "^4.17.15",
		"comment-json": "2.1.0",
		"fs-extra": "7.0.1"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.150",
		"@types/comment-json": "1.1.1",
		"@types/fs-extra": "5.0.5",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vsce": "^1.69.0",
		"vscode-test": "^1.2.2"
	}
}
